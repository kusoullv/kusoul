<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.maqh.demo.dao.ss.CommentMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.maqh.demo.domain.ss.Comment">
                    <result column="id" property="id"/>
                    <result column="comment" property="comment"/>
                    <result column="vidoID" property="vidoID"/>
                    <result column="photoID" property="photoID"/>
                    <result column="commentID" property="commentID"/>
                    <result column="commentDate" property="commentDate"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, comment, vidoID, photoID, commentID, commentDate
        </sql>

</mapper>